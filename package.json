{
  "name": "static-site-generator",
  "description": "Static site generator / project skeleton for creating production-ready sites.",
  "keywords": [
    "static-site-generator",
    "pug",
    "gulp",
    "linting",
    "restful",
    "continuous-integration",
    "babel",
    "sass",
    "generator",
    "template",
    "project-scaffolding",
    "project-skeleton"
  ],
  "author": "Simon Knittel <hallo@simonknittel.de> (https://simonknittel.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonknittel/static-site-generator"
  },
  "bugs": {
    "url": "https://github.com/simonknittel/static-site-generator/issues"
  },
  "scripts": {
    "test": "npm run test:js && npm run production && npm run test:html",
    "test:js:tslint": "tslint -p tsconfig.json -c tslint.json",
    "test:js:eslint": "eslint \"src/**/*.ts\"",
    "test:js:lint": "npm run test:js:tslint & npm run test:js:eslint",
    "test:js:jest": "jest",
    "test:js": "npm run test:js:lint & npm run test:js:jest",
    "test:html": "html-validator --file=dist/index.html --verbose",
    "postinstall": "npm rebuild node-sass",
    "start": "gulp watch",
    "default": "gulp",
    "production": "gulp production",
    "nuke": "rm -rf node_modules dist"
  },
  "engines": {
    "node": ">=10.13.0 <=11",
    "npm": ">=6.4.1"
  },
  "devDependencies": {
    "browser-sync": "2.26.7",
    "compression": "1.7.4",
    "connect-modrewrite": "0.10.2"
  },
  "dependencies": {
    "@types/jest": "24.0.15",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "critical": "1.3.4",
    "del": "5.0.0",
    "eslint": "6.0.1",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "6.1.0",
    "gulp-cached": "1.1.1",
    "gulp-data": "1.3.1",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "6.0.0",
    "gulp-more-css": "1.0.0",
    "gulp-notify": "3.2.0",
    "gulp-plumber": "1.2.1",
    "gulp-pug": "4.0.1",
    "gulp-pug-linter": "1.2.0",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sitemap": "7.3.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-stylelint": "9.0.0",
    "gulp-svg-sprite": "1.5.0",
    "html-validator-cli": "6.0.1",
    "imagemin-gifsicle": "6.0.1",
    "imagemin-jpegtran": "6.0.0",
    "imagemin-optipng": "7.0.0",
    "imagemin-svgo": "7.0.0",
    "jest": "24.8.0",
    "js-yaml": "3.13.1",
    "jstransformer-marked": "1.0.3",
    "node-notifier": "5.4.0",
    "normalize.css": "8.0.1",
    "pug": "2.0.4",
    "stylelint": "10.1.0",
    "stylelint-group-selectors": "1.0.7",
    "stylelint-high-performance-animation": "1.2.1",
    "stylelint-order": "3.0.0",
    "stylelint-scss": "3.9.0",
    "stylelint-selector-no-empty": "1.0.7",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "5.18.0",
    "typescript": "3.5.2",
    "typescript-eslint-parser": "21.0.2",
    "webpack": "4.35.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
