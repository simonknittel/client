{
  "name": "static-site-generator",
  "description": "Start a new static or RESTful page within seconds.",
  "author": "Simon Knittel <hallo@simonknittel.de> (https://simonknittel.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonknittel/static-site-generator"
  },
  "bugs": {
    "url": "https://github.com/simonknittel/static-site-generator/issues"
  },
  "scripts": {
    "_test": "npm run test:js && npm run production && npm run test:html && npm run test:visual",
    "test": "npm run test:js && npm run production",
    "test:js": "jest",
    "test:html": "babel-node valimate",
    "test:visual": "backstop test",
    "postinstall": "npm rebuild node-sass",
    "start": "gulp watch",
    "default": "gulp",
    "production": "NODE_ENV=production gulp production",
    "nuke": "rm -rf node_modules build"
  },
  "engines": {
    "node": ">=6.11.1 <=8.1.3",
    "npm": ">=3.10.10 <=5.0.3",
    "yarn": "0.27.5"
  },
  "devDependencies": {
    "@types/jest": "20.0.8",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "backstopjs": "3.0.19",
    "browser-sync": "2.18.13",
    "compression": "1.7.0",
    "connect-modrewrite": "0.10.1",
    "critical": "0.9.0",
    "cson": "4.1.0",
    "del": "3.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "4.0.0",
    "gulp-cached": "1.1.1",
    "gulp-data": "1.2.1",
    "gulp-eslint": "4.0.0",
    "gulp-htmlmin": "3.0.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "3.3.0",
    "gulp-kraken": "0.2.0",
    "gulp-more-css": "1.0.0",
    "gulp-notify": "3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-pug": "3.3.0",
    "gulp-pug-lint": "0.1.6",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.6.1",
    "gulp-sass": "3.1.0",
    "gulp-scss-lint": "0.5.0",
    "gulp-sitemap": "4.2.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-svg-sprite": "1.3.7",
    "imagemin-gifsicle": "5.2.0",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-optipng": "5.2.1",
    "imagemin-svgo": "5.2.2",
    "jest": "20.0.4",
    "jstransformer-marked": "1.0.2",
    "node-notifier": "5.1.2",
    "pug": "2.0.0-rc.3",
    "ts-jest": "20.0.10",
    "ts-loader": "2.3.3",
    "typescript": "2.4.2",
    "valimate": "2.3.0",
    "webpack": "3.5.5"
  },
  "dependencies": {
    "normalize.css": "7.0.0"
  },
  "jest": {
    "rootDir": "source",
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
