{
  "name": "static-site-generator",
  "description": "Static site generator to create blazingliy fast production-ready sites.",
  "keywords": [
    "static-site-generator",
    "pug",
    "gulp",
    "linting",
    "restful",
    "continuous-integration",
    "babel",
    "sass",
    "generator",
    "template"
  ],
  "author": "Simon Knittel <hallo@simonknittel.de> (https://simonknittel.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonknittel/static-site-generator"
  },
  "bugs": {
    "url": "https://github.com/simonknittel/static-site-generator/issues"
  },
  "scripts": {
    "_test": "npm run test:js && npm run production && npm run test:html && npm run test:visual",
    "test": "npm run test:js & npm run production",
    "test:js:tslint": "tslint -p tsconfig.json -c tslint.json",
    "test:js:eslint": "eslint \"src/**/*.ts\"",
    "test:js:lint": "npm run test:js:tslint & npm run test:js:eslint",
    "test:js:jest": "jest",
    "test:js": "npm run test:js:lint & npm run test:js:jest",
    "test:html": "babel-node valimate",
    "test:visual": "backstop test",
    "postinstall": "npm rebuild node-sass",
    "start": "gulp watch",
    "default": "gulp",
    "production": "gulp production",
    "nuke": "rm -rf node_modules dist"
  },
  "engines": {
    "node": ">=8.9.0 <=9",
    "npm": ">=5.5.1"
  },
  "devDependencies": {
    "@types/jest": "20.0.8",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "backstopjs": "3.0.32",
    "browser-sync": "2.18.13",
    "compression": "1.7.1",
    "connect-modrewrite": "0.10.2",
    "critical": "1.0.0",
    "cson": "4.1.0",
    "del": "3.0.0",
    "eslint": "4.10.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "4.0.0",
    "gulp-cached": "1.1.1",
    "gulp-data": "1.2.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-imagemin": "3.4.0",
    "gulp-kraken": "0.2.0",
    "gulp-more-css": "1.0.0",
    "gulp-notify": "3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-pug": "3.3.0",
    "gulp-pug-lint": "0.1.6",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.6.1",
    "gulp-sass": "3.1.0",
    "gulp-scss-lint": "0.5.0",
    "gulp-sitemap": "4.2.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-svg-sprite": "1.3.7",
    "imagemin-gifsicle": "5.2.0",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-optipng": "5.2.1",
    "imagemin-svgo": "6.0.0",
    "jest": "21.2.1",
    "jstransformer-marked": "1.0.2",
    "node-notifier": "5.1.2",
    "pug": "2.0.0-rc.4",
    "ts-jest": "21.2.1",
    "ts-loader": "3.1.1",
    "tslint": "5.8.0",
    "typescript": "2.6.1",
    "typescript-eslint-parser": "8.0.1",
    "valimate": "2.3.0",
    "webpack": "3.8.1"
  },
  "dependencies": {
    "normalize.css": "7.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
