{
  "name": "static-site-generator",
  "description": "Start a new static or RESTful page within seconds.",
  "author": "Simon Knittel <hallo@simonknittel.de> (https://simonknittel.de)",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonknittel/static-site-generator"
  },
  "bugs": {
    "url": "https://github.com/simonknittel/static-site-generator/issues"
  },
  "scripts": {
    "test": "npm run test:js; gulp && npm run test:html && npm run test:visual",
    "test:js": "jest source/assets/scripts",
    "test:html": "babel-node valimate",
    "test:visual": "backstop test",

    "postinstall": "npm rebuild node-sass && jspm install",

    "start": "gulp watch",
    "default": "gulp",
    "production": "gulp production",

    "nuke": "rm -rf node_modules jspm_packages build"
  },
  "engines": {
    "node": ">=6.10.0 <=7.7.2",
    "npm": ">=3.10.10 <4.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.23.1",
    "babel-jest": "19.0.0",
    "babel-preset-es2015": "6.22.0",
    "backstopjs": "2.6.11",
    "browser-sync": "2.18.8",
    "compression": "1.6.2",
    "connect-modrewrite": "0.10.1",
    "critical": "0.8.4",
    "cson": "4.0.0",
    "del": "2.2.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "3.1.1",
    "gulp-cached": "1.1.1",
    "gulp-data": "1.2.1",
    "gulp-eslint": "3.0.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "3.1.1",
    "gulp-jspm": "0.5.13",
    "gulp-kraken": "0.2.0",
    "gulp-more-css": "1.0.0",
    "gulp-notify": "3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-pug": "3.3.0",
    "gulp-pug-lint": "0.1.6",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-sass": "3.1.0",
    "gulp-scss-lint": "0.4.0",
    "gulp-sftp": "0.1.5",
    "gulp-sitemap": "4.2.0",
    "gulp-sourcemaps": "2.4.1",
    "gulp-svg-sprite": "1.3.6",
    "imagemin-gifsicle": "5.1.0",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-optipng": "5.2.1",
    "imagemin-svgo": "5.2.0",
    "jest": "19.0.2",
    "jspm": "0.16.52",
    "jstransformer-marked": "1.0.2",
    "pug": "2.0.0-beta11",
    "valimate": "2.3.0"
  },
  "dependencies": {
    "normalize.css": "5.0.0"
  },
  "jspm": {
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
